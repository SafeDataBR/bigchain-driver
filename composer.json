{
    "name": "safedatabr/bigchain-driver",
    "description": "BigChainDB PHP Driver",
    "authors": [
        {
            "name": "Vinicius Vicente",
            "email": "vinyvicente@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "library",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "BigChainDB\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BigChainDB\\": "src",
            "BigChainDB\\Tests\\": "tests"
        }
    }
}
